{
  "extends": [
    "config:base",
    "schedule:monthly"
  ],
  "packageRules": [
    {
      "packagePatterns": [
        "gatsby"
      ],
      "groupName": "gatsby"
    },
    {
      "packageNames": [
        "react",
        "react-dom"
      ],
      "groupName": "react"
    },
    {
      "packagePatterns": [
        "@material-ui"
      ],
      "groupName": "material-ui"
    }
  ],
  "labels": [
    "renovate"
  ],
  "prHourlyLimit": 4,
  "prConcurrentLimit": 16,
  "rangeStrategy": "bump",
  "bumpVersion": null,
  "semanticCommits": true
}
